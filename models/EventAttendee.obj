Model EventAttendee {
	name : 'Event Attendee'

	properties :[
		Event {
			type : #Reference
			referenceTo : #Event
		}
		member {
			type : #Reference
			referenceTo : #Member
		},
		email {
			type : #Email
		},
		phone {
			type : #Phone
		},
		role {
			type : #Reference
			referenceTo : #EventRole
		},
		invitation {
			type : #Reference
			referenceTo : #Invitation
		},
		checkIn {
			type : #DateTime
		},
		checkOut {
			type : #DateTime
		}
	]
}