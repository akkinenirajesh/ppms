Model Invitation{
	name :'Invitation'
	properties:[
		activity{
			type:#Reference
			referenceTo:#Activity
		},
		from{
			type:#Reference
			referenceTo:#ActivityAttendee
		},
		toMember{
			type:#Reference
			referenceTo:#Member
		},
		toEmail{
			type:#Email
		},
		personalMessage{
			type:#Text
		},
		response{
			type:#Reference
			referenceTo:#InvitationResponse
		},
		responseDate{
			type:#Date
		},
		role{
			type:#Reference
			referenceTo:#ActivityRole
		},
		reportTo{
			type:#Reference
			referenceTo:#ActivityAttendee
		}
	]
}