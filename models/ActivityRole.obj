
// used to define roles of users participating in the event
// this can be used by other to decide whom to contact
Model ActivityRole {
	name : 'Activity Role'
	properties : [
		role {
			type : #Text
			required : true
		},
		member {
			type : #Reference
			referenceType : #Member
		},
		reportsTo {
			type : #Reference
			referenceType : #ActivityRole
		}
	]
}