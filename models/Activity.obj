// defines an activity or event
Model Activity {
	name : 'Activity'
	properties : [
		title {
			type : #Text
			required : true
		},
		description {
			type : #Text
			required : true
		},
		attendees {

		},
		invitees {

		},
		manager {
			
		},
		date {

		},
		location {

		},
		venue {

		},
		project {

		},
		team {
			type : #Reference
			referenceType : #ActivityRole
			relation : #OneToManyChild
		}
	]
}
